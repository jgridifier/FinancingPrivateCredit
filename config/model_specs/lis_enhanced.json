{
  "name": "lis_enhanced",
  "description": "APLR model enhanced with Lending Intensity Score (LIS) as leading indicator",
  "target": "provision_rate",
  "ar_lags": [1, 2, 3, 4],
  "exogenous_vars": ["lis", "loan_growth_yoy"],
  "exogenous_lags": {
    "lis": [4, 8, 12],
    "loan_growth_yoy": [1, 4]
  },
  "include_seasonality": true,
  "seasonality_period": 4,
  "max_bins": 15,
  "min_observations_in_split": 4,
  "max_interaction_level": 1,
  "holdout_periods": 4,
  "bank_fixed_effects": false
}
